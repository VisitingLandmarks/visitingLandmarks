{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "standard",
    "standard-react"
  ],
  "installedESLint": true,
  "plugins": [
    "json",
    "react"
  ],
  "globals": {
    "__dirname": true,
    "process": true,
    "L":true
  },
  "rules": {
    "space-before-function-paren": [
      "error",
      "always"
    ],
    "spaced-comment": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-var": "error",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "react/jsx-indent": [
      2,
      4
    ],
    "react/jsx-indent-props": [
      2,
      4
    ]
  }
}